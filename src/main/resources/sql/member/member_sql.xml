<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kafka_test.dao.MemberDao">

    <select id="getMemberList" resultType="com.kafka_test.dto.MemberDto">
    	SELECT
       		MBR_ID AS mbrId
       		,MBR_NM AS mbrNm
   		FROM 
   			MEMBER_MST
    </select>
    
</mapper>
